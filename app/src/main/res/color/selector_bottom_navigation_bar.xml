<?xml version="1.0" encoding="utf-8"?>
<!-- Icon color selection for the bottom navigation bar -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true"  android:color="@color/colorWhite" />
    <item  android:state_checked="false" android:state_enabled="true" android:color="@color/colorWhiteMediumEmphasis" />
    <item  android:state_enabled="false" android:color="@color/colorWhiteDisabled" />
</selector>